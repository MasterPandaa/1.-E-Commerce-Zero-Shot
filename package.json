{
  "name": "ecommerce-express-mysql",
  "version": "1.0.0",
  "description": "Full-stack E-Commerce app using Node.js, Express, MySQL, and JavaScript",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "node",
    "express",
    "mysql",
    "ecommerce"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-mysql-session": "^2.1.10",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mime": "^4.0.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "stripe": "^16.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "optionalDependencies": {
    "sharp": "^0.33.5"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "views"
    ],
